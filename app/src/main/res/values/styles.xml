<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />

    <style name="AppTheme.AppBarOverlay" parent="AppTheme">
        <item name="homeAsUpIndicator">@mipmap/nav_back_b</item>
        <item name="android:homeAsUpIndicator">@mipmap/nav_back_b</item>
        <item name="navigationIcon">@mipmap/nav_back_b</item>
        <item name="actionMenuTextColor">@color/white</item>
    </style>

</resources>
